<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Send.Api.Mobile.Main"
             xmlns:local="clr-namespace:Send.Api.Mobile"
            x:Name="tbMain">
    <!--Pages can be added as references or inline-->
    <ContentPage  x:Name="home" Title="Home">
        <StackLayout>
            <Image Aspect="Fill" Margin="20">
                <Image.Source>
                    <UriImageSource Uri="{local:ImageResource Send.Api.Mobile.Images.IMG_20200103_002852_833.jpg}"></UriImageSource>
                </Image.Source>
            </Image>
        </StackLayout>
    </ContentPage>
</TabbedPage>